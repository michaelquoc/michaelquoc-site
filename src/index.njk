<script>
    const SUBSTACK_URL = "https://thefirstprinciple.io/feed";
    const CONTAINER_ID = "codex-container";
    const MAX_POSTS = 5;

    document.addEventListener("DOMContentLoaded", fetchRSS);

    async function fetchRSS() {
        try {
            const response = await fetch(`https://api.rss2json.com/v1/api.json?rss_url=${encodeURIComponent(SUBSTACK_URL)}`);
            const data = await response.json();
            if (data.status === 'ok') { renderCodex(data.items); }
        } catch (error) {
            console.error('Codex Offline:', error);
            document.getElementById(CONTAINER_ID).innerHTML = `<p class="mono-label" style="opacity:0.5;">SYSTEM_OFFLINE</p>`;
        }
    }

    function renderCodex(posts) {
        const container = document.getElementById(CONTAINER_ID);
        container.innerHTML = ''; 

        posts.slice(0, MAX_POSTS).forEach((post, index) => {
            const date = new Date(post.pubDate).toLocaleDateString('en-US', { month: 'short', day: 'numeric' });
            const html = `
                <div style="padding: 1.5rem 0; border-bottom: 1px solid var(--border-alpha);">
                    <div style="display:flex; justify-content:space-between; margin-bottom:0.5rem;">
                        <span class="mono-label" style="color: var(--scuderia-red);">SIGNAL_${String(index).padStart(2, '0')}</span>
                        <span class="mono-label" style="opacity:0.5;">${date}</span>
                    </div>
                    <h4 style="font-size: 1.2rem; font-weight: 500;">
                        <a href="${post.link}" target="_blank" style="text-decoration:none; color: #FFF;">${post.title} -></a>
                    </h4>
                </div>
            `;
            container.innerHTML += html;
        });
    }
</script>